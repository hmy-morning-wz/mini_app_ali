page {
    width: 100%;
    background: #fff;
    overflow-x: hidden;
    overflow-y: scroll;
    height: 100%;
}

.am-wingblank.am-wingblank-sm {
    margin-left: 0.1rem;
    margin-right: 0.1rem;
}

.amount_title {
    font-family: Helvetica;
    font-size: 0.54rem;
    color: #000000;
    line-height: 0.65rem;
    text-align: center;
    padding-top: 1.2rem;
}

.order_state {
    font-family: PingFangSC-Regular;
    font-size: 0.24rem;
    line-height: 0.4rem;
    color: #888888;
    text-align: center;
    margin-bottom: 1rem;
    margin-top: 0.1rem;
}

.line_ul {
    font-family: PingFangSC-Regular;
    font-size: 0.28rem;
    color: #888888;
    list-style: none;
    width: 100%;
    margin-left: 0.2rem;
}

.line_li {
    float: left;
    width: 100%;
    padding: 0.05rem 0 0.2rem 0;
    line-height: 0.48rem;
    display: list-item;
    text-align: -webkit-match-parent;
}

.line_title {
    float: left;
    text-align: left;
    width: 25%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.line_text {
    float: right;
    text-align: right;
    width: 65%;
    white-space: nowrap;
    margin-right: 0.5rem;
    overflow: hidden;
    text-overflow: ellipsis;
}