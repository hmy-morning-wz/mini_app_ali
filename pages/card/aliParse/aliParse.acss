.aliParse-u {
  text-decoration: underline;
}
.aliParse-hide{
    display: none;
}
.AliEmojiView{
    align-items: center;
}
.aliEmoji{
    width: 16px;
    height:16px;
}
.aliParse-tr{
	display: flex;
	border-right:1px solid #e0e0e0;
	border-bottom:1px solid #e0e0e0;
	border-top:1px solid #e0e0e0;
}
.aliParse-th,
.aliParse-td{
	flex:1;
	padding:5px;
	font-size:28rpx;
	border-left:1px solid #e0e0e0;
	word-break: break-all;
}
.aliParse-td:last{
	border-top:1px solid #e0e0e0;
}
.aliParse-th{
	background:#f0f0f0;
	border-top:1px solid #e0e0e0;
}
.aliParse-del{
    display: inline;
}
.aliParse-figure {
  overflow: hidden;
}
